{
    "name": "Data Analysis Env",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "settings": {
      "python.pythonPath": "/usr/local/bin/python",
      "jupyter.jupyterServerType": "local"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter"
    ],
    "forwardPorts": [8888],
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "vscode"
  }